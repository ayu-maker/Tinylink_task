generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Link {
  code        String   @id @unique @db.VarChar(8)
  url         String   @db.Text
  clicks      Int      @default(0)
  lastClicked DateTime?
  createdAt   DateTime @default(now())
}
